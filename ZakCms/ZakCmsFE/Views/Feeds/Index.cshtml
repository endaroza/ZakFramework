@using System.Globalization
@model IEnumerable<ZakCms.Models.Entitites.FeedModel>
<table>
	<tr>
		<th>
			Title
		</th>
		<th>
			Language
		</th>
		<th>
			CreateTime
		</th>
		<th>
		</th>
	</tr>
	@foreach (var item in Model)
	{
		<tr>
			<td>
				@Html.DisplayFor(modelItem => item.Title)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.Language.Description)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.CreateTime)
			</td>
			<td>
				
				@Html.ActionLink("Feed Content", "Details", new {langId = item.Language.Code, id = string.IsNullOrEmpty(item.SeoTitle) ? item.Id.ToString(CultureInfo.InvariantCulture) : item.SeoTitle})
			</td>
		</tr>
	}
</table>