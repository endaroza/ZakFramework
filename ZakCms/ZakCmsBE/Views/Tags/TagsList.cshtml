@model IEnumerable<ZakCms.Models.Entitites.TagModel>
<table>
	<tr>
		<th>
			Code
		</th>
		<th>
			Description
		</th>
	</tr>
	@foreach (var item in Model)
	{
		<tr>
			<td>
				@Html.ActionLink(item.Code, "Index", new {id = item.Id, view = "Details"})
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.Description)
			</td>
		</tr>
	}
</table>