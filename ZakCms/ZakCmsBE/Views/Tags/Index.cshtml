@model ZakCms.Models.PageTagAdminViewModel
@{
	Layout = "~/Views/Shared/_Layout.cshtml";
}
<h3>
	Tags Administration</h3>
<table>
	<tr>
		<td>
			<p>
				@Html.ActionLink("Add Tag", "Index", new {id = 0, view = "Create"})
			</p>
			<div class="menuWidth">
				@{ Html.RenderPartial("TagsList", Model.Tags); }
			</div>
		</td>
		<td>
			@{ Html.RenderPartial(Model.View, Model.Tag); }
		</td>
	</tr>
</table>